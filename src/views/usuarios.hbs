            </td>
            <td>
                
                {{identificacion}}
                
            </td>
            <td class="small_visible">
                {{nombre}}
                
            </td>
            <td class="small_visible">
                {{apellido}}
            </td>
            <td>
                
                {{telefono}}
            </td>
            <td>
                {{eps}}
            </td>
            <td>
                {{fecha_ingreso}}
            </td>
        </tr>
        
        
        {{/each}}

        </tbody>
        
    </table>
